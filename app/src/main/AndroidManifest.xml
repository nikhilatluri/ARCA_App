<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.arca">

    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".ToHome"
        android:allowBackup="true"
        android:icon="@drawable/arca_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/arca_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.ARCA">
        <activity android:name=".Feed" />
        <activity android:name=".Expences" />
        <activity android:name=".Sales" />
        <activity android:name=".UserCreation" />
        <activity android:name=".Home" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>